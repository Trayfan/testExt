{
    "manifest_version": 3,
    "name": "Combined Extension",
    "version": "1.0",
    "description": "Adds buttons to manage environments and send text to server.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://allure.vsk.ru/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}